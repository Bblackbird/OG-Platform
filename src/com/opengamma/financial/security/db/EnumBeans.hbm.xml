<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.opengamma.financial.security.db">
  <class name="CurrencyBean" table="currencies">
    <id name="id" column="currency_id">
      <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
    </id>
    <property name="name" unique="true" type="string" not-null="true"/>
  </class>
  <class name="ExchangeBean" table="exchanges">
    <id name="id" column="exchange_id">
      <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
    </id>
    <property name="name" unique="true" type="string" not-null="true"/>
    <property name="description" type="string"/>
  </class>
</hibernate-mapping>