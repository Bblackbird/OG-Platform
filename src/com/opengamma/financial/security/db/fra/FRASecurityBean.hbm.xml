<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.opengamma.financial.security.db.fra">
  <union-subclass name="FRASecurityBean" extends="com.opengamma.financial.security.db.SecurityBean" table="sec_fra">
    <component name="startDate" class="com.opengamma.financial.security.db.ZonedDateTimeBean">
      <property name="date" column="start_date" type="timestamp" not-null="true" />
      <property name="zone" column="start_zone" type="string" not-null="true" />
    </component>
    <component name="endDate" class="com.opengamma.financial.security.db.ZonedDateTimeBean">
      <property name="date" column="end_date" type="timestamp" not-null="true" />
      <property name="zone" column="end_zone" type="string" not-null="true" />
    </component>
  </union-subclass>
  <query name="FRASecurityBean.all">
    from FRASecurityBean
  </query>
</hibernate-mapping>