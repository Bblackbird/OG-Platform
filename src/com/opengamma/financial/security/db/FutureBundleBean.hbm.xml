<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.opengamma.financial.security.db">
  <class name="FutureBundleBean" table="sec_futurebundle">
    <id name="id">
      <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
    </id>
    <many-to-one name="futureSecurity" column="future_id" class="FutureSecurityBean" not-null="true" />
    <property name="startDate" type="date" />
    <property name="endDate" type="date" />
    <property name="conversionFactor" type="double" not-null="true" />
    <set name="basket" inverse="true" cascade="all-delete-orphan">
      <key column="bundle_id" />
      <one-to-many class="FutureBundleIdentifierBean" />
    </set>
  </class>
</hibernate-mapping>