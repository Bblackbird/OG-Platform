<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.opengamma.engine.batch.db">

  <class name="LiveDataSnapshotEntry" table="rsk_live_data_snapshot_entry">
    <id name="id">
      <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
      	<param name="optimizer">hilo</param>
      	<param name="increment_size">100</param>
      </generator>
    </id>
    <many-to-one name="snapshot" 
    	column="snapshot_id" 
    	not-null="true"/>
  	<many-to-one name="identifier" 
    	column="live_data_identifier_id" 
    	not-null="true"/>
  	<many-to-one name="field" 
    	column="field_id" 
    	not-null="true"/>
    <property name="value" not-null="true"/>
  </class>
  
</hibernate-mapping>