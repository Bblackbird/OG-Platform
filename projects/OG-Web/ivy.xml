<ivy-module version="2.0">
    <info organisation="com.opengamma" module="og-web" status="integration"/>
    <configurations>
      <conf name="core"/>
      <conf name="opengamma" extends="core"/>
    </configurations>
    
    <publications>
      <artifact name="og-web" type="jar"/>
    </publications>
    <dependencies>
        <dependency name="og-financial" rev="latest.integration" conf="opengamma->*"/>
        <dependency name="og-analytics" rev="latest.integration" conf="opengamma->*"/>
        <dependency name="og-engine" rev="latest.integration" conf="opengamma->*"/>
        <dependency name="og-util" rev="latest.integration" conf="opengamma->*"/>
        
        <exclude org="org.junit" />
    </dependencies>
</ivy-module>
