<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="aggregatorNamesResource" class="com.opengamma.web.server.push.AggregatorNamesResource">
    <constructor-arg ref="viewDefinitionManager"/>
  </bean>

  <bean id="viewDefinitionManager" class="com.opengamma.web.server.push.AggregatorNamesResourceTest$AggregatedViewDefinitionManagerFactoryBean"/>

  <bean id="updateManager" class="com.opengamma.web.server.push.TestRestUpdateManager"/>

</beans>