<?xml version="1.0" encoding="UTF-8"?>
<og-portfolio schemaVersion="1.0">

  <trades>

    <!-- Trade id must be unique in this list -->
    <swapTrade id="idForThisTradeInThisFile">
      <!-- Probably should be unique in this file too -->
      <externalId>IdFromExternalSystem</externalId>
      <tradeDate>2013-01-20</tradeDate>
      <counterparty><id scheme="Cpty">GOLDMAN</id></counterparty>

      <!--optional-->
      <isCleared>false</isCleared>
      <!--optional-->
      <clearingBroker>dependency on isCleared? / Id format??</clearingBroker>
      <!--optional-->
      <clearingHouse>dependency on isCleared? / Id format??</clearingHouse>
      <!--optional-->
      <executingBroker>Id format??</executingBroker>
      <!--optional-->
      <primeBroker>Id format??</primeBroker>
      <!--optional-->
      <collateralAgreement>Only if not cleared</collateralAgreement>

      <!-- Additional trade attributes - generally data that won't affect analytics -->
      <tradeAttributes>
        <entry key="STRING">STRING</entry>
      </tradeAttributes>

      <tradeLegs>

        <fixedLeg>
          <payReceive>Pay</payReceive>
          <currency>USD</currency>
          <!-- Notional could be array-valued -->
          <notional>1000000</notional>
          <frequency></frequency>
          <effectiveDate>2013-02-01</effectiveDate>
          <maturity>2018-01-31</maturity>
          <rate>105.25</rate>
          <dayCount></dayCount>
          <businessDayAdjustment></businessDayAdjustment>
          <interestCalculation>Adjusted</interestCalculation>
          <scheduleGenerationDirection>Backward</scheduleGenerationDirection>
          <endOfMonth>True</endOfMonth>
          <paymentCalendars>
            <calendar type="Bank"> <!-- default=BANK  -->
              <id scheme="CountryISO2">FD</id>
            </calendar>
            <calendar type="Bank"> <!-- default=BANK  -->
              <id scheme="CountryISO2">EN</id>
            </calendar>
          </paymentCalendars>
          <isIMM>True</isIMM>
          <!--default = short-->
          <shortLongCoupon>Long</shortLongCoupon>
        </fixedLeg>
        <floatingRateLeg type="InterestRate|Inflation">
          <payReceive>Receive</payReceive>
          <currency>USD</currency>
          <!-- Notional could be array-valued -->
          <notional>1000000</notional>
          <!-- map to our InterestRateNotional where sign = Pay/Recieve flag -->
          <frequency> <!-- values from SimpleFrequencyFactory -->
          </frequency>
          <!-- loader needs to enforce against convention for now -->
          <effectiveDate>2013-02-01</effectiveDate>
          <!-- loader should enforce same maturity on each leg for now -->
          <maturity>2018-01-31</maturity>
          <!-- loader should support either spread or gearing for now, schema should allow both -->
          <spread>5.25</spread>
          <gearing>-3</gearing>
          <!-- use DayCountFactory -->
          <dayCount>
          </dayCount>
          <!-- use BusinessDayConventionFactory -->
          <businessDayConvention>Modified Following</businessDayConvention>
          <!-- only adjusted supported now -->
          <interestCalculation>Adjusted</interestCalculation>
          <!-- only Backward supported -->
          <scheduleGenerationDirection>Backward</scheduleGenerationDirection>
          <!-- EOM -->
          <endOfMonth>True</endOfMonth>
          <!-- must be false for now -->
          <isIMM>True</isIMM>


          <paymentCalendars>
            <calendar type="Bank"> <!-- default=BANK  -->
              <id scheme="CountryISO2">FD</id>
            </calendar>
            <calendar>
              <id scheme="CountryISO2">EN</id>
            </calendar>
            <!-- loader should enforce type=BANK for now and id schemes all the same for now -->
          </paymentCalendars>

          <!-- always short for now -->
          <shortLongCoupon>Long</shortLongCoupon>
          <!-- same as frequency for now -->
          <resetFrequency></resetFrequency>
          <!-- not supported at the moment as frequencies must match -->
          <compoundingMethod></compoundingMethod>
          <!-- we're assuming that resetLag == settlementLag at the moment -->
          <resetLag></resetLag>
          <!-- In Advance or In Arrears.  Only support In advance at the mo -->
          <resetType>In Advance</resetType>
          <!-- same as payment calendar at the moment -->
          <fixingCalendar></fixingCalendar>
          <fixingIndex>
            <id scheme="BLOOMBERG_TICKER">US0003M Curncy</id>
          </fixingIndex>
          <!-- Interpolated -->
          <shortLongCouponAdjustment></shortLongCouponAdjustment>
        </floatingRateLeg>


      </tradeLegs>
    </swapTrade>
  </trades>
</og-portfolio>