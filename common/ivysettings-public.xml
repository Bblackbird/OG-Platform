<ivysettings>
  <credentials host="ivyrepo.opengamma.com" username="opengamma" passwd="opengamma" realm="secure"/>
  <resolvers>
    <chain name="public" returnFirst="true">
      <url name="opengamma-ivyrepo">
        <ivy pattern="http://ivyrepo.opengamma.com/[organization]/[module]/[revision]/ivy.xml" />
        <artifact pattern="http://ivyrepo.opengamma.com/[organization]/[module]/[revision]/[artifact]-[revision].[ext]" />
      </url>
      <url name="fudgemsg-org-dist">
        <ivy pattern="http://dist.fudgemsg.org/java/ivy/[organization]/[module]/[revision]/ivy.xml" />
        <artifact pattern="http://dist.fudgemsg.org/java/ivy/[organization]/[module]/[revision]/[artifact]-[revision].[ext]" />
      </url>
    </chain>
  </resolvers>
</ivysettings>
